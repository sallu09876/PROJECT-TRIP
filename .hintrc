{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "video[playsinline]",
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "float: inline-end",
          "scrollbar-width",
          "scrollbar-width: none",
          "-ms-filter",
          "-moz-backface-visibility"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "meta-viewport": "off"
  }
}